{
  "profiles": {
    "primary": [0.3, 0.05, 0.16, 0.05, 0.016],
    "reveal":  [0.6, 0.1, 0.2, 0.1, 0.03],
    "compact": [0.22, 0.05, 0.16, 0.05, 0.016]
  },
  "timelines": {
    "text.charIn": {
      "profile": "primary",
      "targets": ".char .n",
      "stagger": 0.05,
      "from": { "opacity": 0, "yPercent": 100 },
      "to":   { "opacity": 1, "yPercent": 0 },
      "ease": "power4.inOut"
    },
    "text.charOut": {
      "profile": "primary",
      "targets": ".char .n",
      "stagger": 0.04,
      "to":   { "opacity": 0, "yPercent": -20 },
      "ease": "power4.inOut"
    },
    "text.wordIn": {
      "profile": "reveal",
      "targets": ".word",
      "stagger": 0.06,
      "from": { "opacity": 0, "yPercent": 40 },
      "to":   { "opacity": 1, "yPercent": 0 },
      "ease": "power4.inOut"
    },
    "text.lineIn": {
      "profile": "reveal",
      "targets": ".line",
      "stagger": 0.08,
      "from": { "opacity": 0, "yPercent": 20 },
      "to":   { "opacity": 1, "yPercent": 0 },
      "ease": "power4.inOut"
    }
  },
  "labels": {
    "home.hero.titleA.in": "text.charIn",
    "home.hero.titleB.in": "text.charIn",
    "home.hero.kicker.in": "text.lineIn",
    "home.hero.links.in":  "compact"
  }
}